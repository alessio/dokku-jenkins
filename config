export JENKINS_IMAGE=${JENKINS_IMAGE:="jenkins"}
export JENKINS_IMAGE_VERSION=${JENKINS_IMAGE_VERSION:="latest"}
export JENKINS_ROOT=${JENKINS_ROOT:="/var/lib/dokku/services/jenkins"}

export PLUGIN_COMMAND_PREFIX="jenkins"
export PLUGIN_DATA_ROOT=$JENKINS_ROOT
export PLUGIN_ADMIN_USER=admin
export PLUGIN_PASSWORD_FILE="${PLUGIN_DATA_ROOT}/PASSWORD"
export PLUGIN_PORT=8080
export PLUGIN_DEFAULT_ALIAS="JENKINS"
export PLUGIN_ALT_ALIAS="DOKKU_JENKINS"
export PLUGIN_IMAGE=$JENKINS_IMAGE
export PLUGIN_IMAGE_VERSION=$JENKINS_IMAGE_VERSION
export PLUGIN_SCHEME="jenkins"
export PLUGIN_SERVICE="Jenkins"
